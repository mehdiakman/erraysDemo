package sesliHarfler;

public class sesliHarfler {

	public static void main(String[] args) {
		char harf = 'A';

		switch (harf) {
		case 'A':
		case 'I':
		case 'O':
		case 'U':
			System.out.println("Kalın sesli haf");
			break;
		switch (harf) {
		
		}
		case 'E':
		case 'İ':
		case 'Ö':
		case 'Ü':
		default:
			System.out.println("İnce sesli harf");

		}

	}

}
