package sesliHarfler;

public class sesliHarfler {

	public static void main(String[] args) {
		char harf = 'Z';

		switch (harf) {
		case 'A':
		case 'I':
		case 'O':
		case 'U':
			System.out.println("Kalın sesli haf");
			break;
		
		case 'E':
		case 'İ':
		case 'Ö':
		case 'Ü':
			System.out.println("İnce Sesli harf");
		default:
			System.out.println("Geçersiz Karakter Seçimi");

		}

	}

}
